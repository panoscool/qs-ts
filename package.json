{
	"name": "qs-ts",
	"version": "0.1.5",
	"license": "MIT",
	"description": "A TypeScript library to parse and stringify URL query strings.",
	"funding": "https://github.com/sponsors/panoscool",
	"homepage": "https://github.com/panoscool/qs-ts#readme",
	"author": {
		"name": "Panos Kullolli",
		"email": "lkullolli@gmail.com",
		"url": "https://panoscool.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/panoscool/qs-ts.git"
	},
	"bugs": {
		"url": "https://github.com/panoscool/qs-ts/issues"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"test": "bun test",
		"build": "bun build.ts",
		"prepublishOnly": "npm run build",
		"release": "bash ./scripts/release.sh",
		"lint": "biome check",
		"format": "biome format --write"
	},
	"engines": {
		"node": ">=24"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"qs-ts",
		"query",
		"string",
		"params",
		"parameter",
		"url",
		"parse",
		"stringify",
		"encode",
		"decode",
		"querystring",
		"searchparams",
		"urlsearchparams"
	],
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@types/bun": "^1.3.5",
		"typescript": "^5.9.3"
	}
}
